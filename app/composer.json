{
    "type": "project",
    "name": "captivenet/app",
    "require": {
        "mobiledetect/mobiledetectlib": "^4.8",
        "php-di/php-di": "^7.0",
        "slim/psr7": "*",
        "slim/slim": "*",
        "slim/twig-view": "^3.4",
        "symfony/console": "^7.2",
        "symfony/error-handler": "^7.2",
        "symfony/string": "^7.2",
        "twig/twig": "*",
        "vlucas/phpdotenv": "^5.6",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {}
    }
}
